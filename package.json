{
  "name": "ngx-datetime-picker",
  "version": "1.2.0",
  "description": "Date, DateTime & Time Picker components that uses native HTML5 components on mobile",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/RenovoSolutions/ngx-datetimepicker"
  },
  "scripts": {
    "start": "npm run serve",
    "serve": "cd ngx-datetimepicker && ng serve",
    "test": "cd ngx-datetimepicker && npm test",
    "test.once": "cd ngx-datetimepicker && npm test --watch=false",
    "setup": "cd ngx-datetimepicker && npm install",
    "dist": "cd ngx-datetimepicker/src/ && tsc",
    "demo": "npm run dist && cd demo && npm install && npm remove ngx-datetimepicker && npm install ../ && ng serve",
    "demo.build": "npm run dist && cd demo && npm remove ngx-datetimepicker && npm install ../ && ng build"
  },
  "author": "Josh Sommer <joshdsommer@gmail.com> (https://twitter.com/_JoshSommer)",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "typescript": "~2.2.2"
  },
  "peerDependencies": {
    "@angular/common": "4.x",
    "@angular/compiler": "4.x",
    "@angular/core": "4.x",
    "@angular/forms": "4.x",
    "@angular/platform-browser": "4.x",
    "@angular/platform-browser-dynamic": "4.x"
  },
  "devDependencies": {
    "@angular/core": "4.x",
    "@angular/compiler":  "4.x",
    "@angular/compiler-cli": "4.x",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "typescript": "2.4.0",
    "rxjs": "5.4.1"
  }
}
